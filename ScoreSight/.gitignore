# before PyInstaller builds the spec file.
*.spec

# Installer logs
pip-log.txt
pip-delete-this-directory.txt

# Unit test / coverage reports
htmlcov/
.tox/
.nox/
.coverage
.coverage.*
.cache
nosetests.xml
coverage.xml
*.cover
*.py,cover
.hypothesis/
.pytest_cache/

# Environments
.env
.venv
env/
venv/
ENV/
env.bak/
venv.bak/

# Spyder project settings
.spyderproject
.spyproject

# Rope project settings
.ropeproject

# mkdocs documentation
/site

# Jupyter Notebook
.ipynb_checkpoints

# IPython
profile_default/
ipython_config.py

# pyenv
.python-version

# PEP 582; used by PDM, Flit, Dephell and Hatch
__pypackages__/

# Celery stuff
celerybeat-schedule
celerybeat.pid

# SageMath parsed files
*.sage.py

# Environments RStudio generates
.Rproj.user/

# VSCode files
.vscode/

# Sublime Text files
*.sublime-project
*.sublime-workspace

# Mac specific files
.DS_Store

# Windows specific files
Thumbs.db

# Application Specific - Exclude data and report contents
# These are already handled by .gitignore in data/ and reports/
# but adding them here ensures they aren't tracked if those files
# are accidentally deleted.
/data/*
!/data/.gitignore
/reports/*
!/reports/.gitignore